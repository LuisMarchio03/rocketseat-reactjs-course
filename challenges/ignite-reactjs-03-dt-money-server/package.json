{
  "name": "dt-money-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify --exit-child src/server.ts",
    "prebuild": "rimraf dist",
    "build": "sucrase ./src -d ./dist --transforms typescript,imports",
    "pre-prod": "rimraf dist && sucrase ./src -d ./dist --transforms typescript,imports",
    "migrate:dev": "npx prisma migrate dev --name init --create-only",
    "migrate:reset": "npx prisma migrate reset",
    "migrate:deploy": "npx prisma migrate deploy"
  },
  "dependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "sucrase": "^3.34.0"
  }
}
